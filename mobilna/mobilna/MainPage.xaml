        <Button Text="Rzuć kośćmi" BackgroundColor="Chocolate" TextColor="White" HorizontalOptions="Center" Margin="0, 10"/>
        <FlexLayout JustifyContent="SpaceEvenly" x:Name="Photos" BackgroundColor="white">
        </FlexLayout>
        <Label Text="Wynik tego losowania: 0" HorizontalOptions="Center" Margin="0, 10"/>
        <Label Text="Wynik gry: 0" HorizontalOptions="Center" Margin="0, 10"/>
